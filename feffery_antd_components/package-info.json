{
  "name": "feffery_antd_components",
  "version": "0.0.1-rc1",
  "description": "Best implementation of Antd components in Plotly Dash.",
  "repository": {
    "type": "git",
    "url": "git://github.com/https://github.com/CNFeffery/feffery-antd-components.git"
  },
  "bugs": {
    "url": "https://github.com/https://github.com/CNFeffery/feffery-antd-components/issues"
  },
  "homepage": "https://github.com/https://github.com/CNFeffery/feffery-antd-components",
  "main": "build/index.js",
  "scripts": {
    "start": "webpack-serve --config ./webpack.serve.config.js --open",
    "validate-init": "python _validate_init.py",
    "prepublishOnly": "npm run validate-init",
    "build:js": "webpack --mode production",
    "build:backends": "dash-generate-components ./src/lib/components feffery_antd_components -p package-info.json --r-prefix 'feffery' --jl-prefix 'feffery'",
    "build:backends-activated": "(. venv/bin/activate || venv\\scripts\\activate && npm run build:py_and_r)",
    "build": "npm run build:js && npm run build:backends",
    "build:activated": "npm run build:js && npm run build:backends-activated"
  },
  "author": "CNFeffery <fefferypzy@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@ant-design/charts": "^1.4.2",
    "@ant-design/icons": "^4.8.0",
    "@landisdesign/use-updatable-state": "github:landisdesign/use-updatable-state",
    "antd": "4.16.12",
    "jsonx": "^1.12.0",
    "ramda": "^0.26.1",
    "react-dnd": "^14.0.5",
    "react-dnd-html5-backend": "^14.1.0",
    "react-highlight-words": "^0.17.0",
    "react-text-loop": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.3.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^3.6.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "npm": "^6.14.18",
    "prop-types": "^15.8.1",
    "react": "^16.14.0",
    "react-docgen": "^4.1.1",
    "react-dom": "^16.14.0",
    "style-loader": "^0.23.1",
    "styled-jsx": "^3.4.7",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-serve": "3.1.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-react-jsx"
    ]
  },
  "engines": {
    "node": ">=8.11.0",
    "npm": ">=6.1.0"
  },
  "directories": {
    "man": "man"
  }
}
